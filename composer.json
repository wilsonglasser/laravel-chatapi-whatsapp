    "require": {
        "php": "^5.6|^7.0|^7.1|^7.2|^7.3|^8.0|^8.1",
        "guzzlehttp/guzzle": "^6.2|^6.5.5|^7.0.1",
        "ext-json": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^4.8.36|^8.5.8|^9.3.3",
